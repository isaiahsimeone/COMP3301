#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>

/*
 * Developed to test whether /dev/acct correctly
 * records and outputs forked PIDs.
 *
 * Prints out PID, forks and prints the childs PID
 *
 * No args.
 */
int main(int argc, char** argv)
{
	printf("Parent PID: %d\n", getpid());

	if (!fork())
		printf("Child PID: %d\n", getpid());
	
	return (0);
}